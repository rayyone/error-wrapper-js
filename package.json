{
  "name": "ry-error-wrapper",
  "description": "Error wrapper for Rayyone React Projects",
  "version": "0.0.3",
  "author": "Rayyone Ltd.",
  "keywords": [
    "rayyone",
    "axios",
    "error handler",
    "error wrapper",
    "sentry",
    "react",
    "react js",
    "react native"
  ],
  "license": "MIT",
  "repository": {
    "url": "git+https://github.com/rayyone/error-wrapper-js.git",
    "type": "git"
  },
  "engines": {
    "node": "12 || 14 || 16 || 17"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
